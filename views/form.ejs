<script>
  function validte(el) {
    $.getJSON("/data/plz_schweiz.json", function (data) {
      var options = data;
      if (el.value == "") return;
      /*for (var i = 0; i < options.length; i++) {
          if(el.value == options[i].value)
          console.log("y")
          el.style.borderColor  = "green";
          return;
      }*/
      var success = false;
      options.forEach(element => {
        if (el.value == element.plz + " - " + element.ort + " - " + element.kanton) success = true;
      });
      console.log(success)
      if (success == true) return el.style.borderColor = "green";
      el.value = ""
      el.style.borderColor = "red";
    });

  }
  $("#form").submit(function (e) {
    e.preventDefault();
    $('.sub').css('display', 'none')
    $.ajax({
      url: '/qr',
      type: 'POST',
      cache: false,
      data: {
        id: id,
        was: was,
        allowed: allowed
      },
      success: function (data) {
        if (data.success == true) {
          window.location = `/qr/${data.jwt}?name=${data.name}`;
        } else {
          $('.sub').css('display', 'block')
          if (data.error == "addr") {
            $('.house_p').css('color', 'red')
            $('.house_p').css('font-size', '120%')
          }
        }
      },
      error: function (jqXHR, textStatus, err) {
        $('.error').css('display', 'block')
      }
    })
  })
</script>